{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
        "@shared": [ "app/shared" ],
        "@shared/*": [ "app/shared/*" ],
        "@core": [ "app/core/" ],
        "@core/*": [ "app/core/*" ],
        "@testing": [ "testing/" ],
        "@testing/*": [ "testing/*" ],
        "@env": [ "environments/" ],
        "@env/*": [ "environments/*" ],
        "@sip": [ "app/routes/sip/" ],
        "@sip/*": [ "app/routes/sip/*" ],
        "@routes": [ "app/routes/" ],
        "@routes/*": [ "app/routes/*" ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "./app/delon/**"
  ]
}
